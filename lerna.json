{
  "version": "1.0.0",
  "npmClient": "npm",
  "npmClientArgs": [
    "--production",
    "--no-optional"
  ],
  "command": {
    "publish": {
      "forcePublish": true,
      "ignoreChanges": [
        "test/",
        "*.md",
        ".gitignore",
        "*.log",
        "*.md",
        "package-lock.json",
        "package-copy.json",
        "node_modules/"
      ],
      "message": "chore(release): publish",
      "registry": "https://jfrog.infra.ww5sawfyut0k.bitsvc.io/artifactory/api/npm/rose-npm-dev-all/"
    },
    "bootstrap": {
      "npmClientArgs": [
        "--no-optional"
      ]
    }
  },
  "packages": [
    "packages/*"
  ]
}